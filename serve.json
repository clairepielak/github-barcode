{
  "headers": [
    {
      "source": "**/*.*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=1"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        }
      ]
    }
  ]
}
